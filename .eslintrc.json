{
    "extends": [
      "airbnb",
      "airbnb/hooks"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": "off",
      "no-console": "warn",
      "import/prefer-default-export": "off",
      "react/react-in-jsx-scope": "off",
      "max-len": [
        "error",
        {
          "code": 160,
          "ignoreUrls": true
        }
      ],
      "import/no-cycle": "off"
    }
  }
  